{
    "collectCoverage": true,
    "collectCoverageFrom": ["source/**/*.js"],
    "coverageDirectory": "../coverage", 
    "coverageThreshold": {
        "global": {
          "branches": 100,
          "functions": 100,
          "lines": 100,
          "statements": 100
        }
    },
    "transform": {
        "^.+\\.js$": "babel-jest"
    }
}